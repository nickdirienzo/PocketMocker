adb shell "echo 'select * from locations where rec_id = 9;' | sqlite3 /data/data/edu.buffalo.cse.blue.pocketmocker/databases/PocketMocker.db" > locations

adb shell "echo 'select * from sensor_events where rec_id = 9;' | sqlite3 /data/data/edu.buffalo.cse.blue.pocketmocker/databases/PocketMocker.db" > sensor_events

adb shell "echo 'select * from wifi_results where rec_id = 9;' | sqlite3 /data/data/edu.buffalo.cse.blue.pocketmocker/databases/PocketMocker.db" > wifi_results

adb shell "echo 'select * from cell_locations where rec_id = 9;' | sqlite3 /data/data/edu.buffalo.cse.blue.pocketmocker/databases/PocketMocker.db" > cell_locations

adb shell "echo 'select * from objectives where rec_id=9;' | sqlite3 /data/data/edu.buffalo.cse.blue.pocketmocker/databases/PocketMocker.db" > objective

adb shell "echo 'select * from recordings where _id=9;' | sqlite3 /data/data/edu.buffalo.cse.blue.pocketmocker/databases/PocketMocker.db" > recording

adb shell "echo 'select * from current_rec_id where rec_id=9;' | sqlite3 /data/data/edu.buffalo.cse.blue.pocketmocker/databases/PocketMocker.db" > current_recording

adb shell "echo 'select * from rec_replay limit 1;' | sqlite3 /data/data/edu.buffalo.cse.blue.pocketmocker/databases/PocketMocker.db" > is_record

